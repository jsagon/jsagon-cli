    "eslint-plugin-promise": "^4.3.1",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.6",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.3"
  },
  "files": [
    "LICENSE",
    "README.md",
    "dist/"
  ],
  "dependencies": {
    "axios": "^0.21.4",
    "chalk": "^4.1.2",
