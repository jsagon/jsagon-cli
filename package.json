    "eslint-plugin-promise": "^4.2.1",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "ts-node-dev": "^1.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  },
  "files": [
    "LICENSE",
    "README.md",
    "dist/"
  ],
  "dependencies": {
    "axios": "^0.21.1",
    "chalk": "^4.1.0",
